{
    "name": "naugrim/laravel-pipeline-tasks",
    "description": "Pipeline Tasks based on illuminate/pipeline",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        },
        {
            "name": "naugrim",
            "email": "laravel-pipeline-tasks@naugrim.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/pipeline": "5.6.*",
        "illuminate/container": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Naugrim\\PipelineTasks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naugrim\\PipelineTasks\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "aliases": {
                "PipelineTasks": "Naugrim\\PipelineTasks\\Facades\\PipelineTasks"
            }
        }
    }
}
